{
  "id": "1",
  "name": "Disable User",
  "method": "PATCH",
  "uri": {
    "scheme": "<<SCHEME>>",
    "host": "<<HOST>>",
    "path": "/v1.0/users/<EL>attributes.get('uid').get(0)</EL>"
  },
  "headers": [
    {
      "name": "Content-Type",
      "value": "application/json"
    },
    {
      "name": "Authorization",
      "value": "<<CREDENTIALS>>"
    }
  ],
  "body": {
    "type": "text",
    "textBody": {
      "accountEnabled": "<EL>attributes.get('status').get(0)</EL>"
    }
  }
}